{
    "name": "anomaly/streams-platform",
    "description": "A drop-in application platform for Laravel.",
    "authors": [{
        "name": "Ryan Thompson",
        "email": "ryan@pyrocms.com"
    }],
    "require": {
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2.0",
        "laravel/helpers": "^1.1.0",
        "laravel/scout": "^7.1.0",
        "intervention/image": "^2.5.0",
        "jenssegers/agent": "2.6.*",
        "laravelcollective/html": "^6.0.0",
        "erusev/parsedown-extra": "0.7.1",
        "mrclay/jsmin-php": "~2.4",
        "tubalmartin/cssmin": "4.0.*",
        "nicmart/string-template": "0.1.*",
        "misd/linkify": "1.1.*",
        "composer/composer": "~1.9.0",
        "ezyang/htmlpurifier": "~4.0",
        "spatie/blink": "~1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\Streams\\Platform\\": "src/"
        },
        "files": [
            "src/Support/_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Anomaly\\Tests\\Streams\\Platform\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anomaly\\Streams\\Platform\\StreamsEventProvider",
                "Anomaly\\Streams\\Platform\\StreamsServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
